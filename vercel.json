{
               "builds": [{
                   "src": "TokenizerA/wsgi.py",
                   "use": "@vercel/python",
                   "config": { "maxLambdaSize": "1024mb", "runtime": "python3.12" }
               }],
               "routes": [
                   {
                       "src": "/(.*)",
                       "dest": "TokenizerA/wsgi.py"
                   }
               ]
           }